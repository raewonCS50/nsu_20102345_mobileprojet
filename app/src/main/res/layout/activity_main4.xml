<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="387dp"
        android:layout_height="693dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="26dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView4_1"
            android:layout_width="match_parent"
            android:layout_height="259dp"
            android:src="@drawable/img_6" />

        <TextView
            android:id="@+id/textView4_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="성심당 튀김소보르" />

        <TextView
            android:id="@+id/textView4_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="https://www.sungsimdangmall.co.kr/product/content.asp?guid=523080" />

        <Button
            android:id="@+id/btn4_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="홈페이지로 가기" />

        <Button
            android:id="@+id/btn4_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="홈화면으로" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>